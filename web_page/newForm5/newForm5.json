      "value" : [
        "if( $data.companiesBidded && $data.companiesBidded_companies ){",
        "\t//attach lazy references variables to parent variables",
        "\t$data.companiesBidded.companies = $data.companiesBidded_companies;",
        "\treturn {",
        "\t\t//map companiesBidded variable to expected task contract input",
        "\t\tcompaniesBiddedInput: {",
        "\t\t\tcompanies: $data.companiesBidded.companies.map( it => ({",
        "\t\t\t\tpersistenceId_string: it.persistenceId_string !== undefined ? it.persistenceId_string : null,",
        "\t\t\t\tname: it.name !== undefined ? it.name : null,",
        "\t\t\t\tcategory: it.category !== undefined ? it.category : null",
        "\t\t\t}))",
        "\t\t}",
        "\t}",
        "}"
      ],
